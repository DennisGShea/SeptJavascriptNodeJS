// RightTriangleTree
 
 // Loop exercise part one
// Draw Right Triangle 
// able to specify height and symbol used

let outputSymbol = '#' ;
let triangleHeight = 6 ;

function RightAngTriangle(triangleHeight, outputSymbol)
  { let aString = outputSymbol;

  for (let i = 0; i < triangleHeight; i++) {
   console.log(aString)
   aString = aString + outputSymbol;
  }
}
RightAngTriangle(triangleHeight, outputSymbol)